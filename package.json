{
  "name": "azure-service-bus-emitter",
  "version": "5.1.0",
  "description": "The Socket.IO Azure Service Bus emitter, allowing to communicate with a group of Socket.IO servers from another Node.js process",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist/"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "npm run format:check && tsc && nyc mocha --require ts-node/register --timeout 5000 test/index.ts --exit",
    "format:check": "prettier --parser typescript --check lib/**/*.ts test/**/*.ts",
    "format:fix": "prettier --parser typescript --write lib/**/*.ts test/**/*.ts",
    "prepack": "tsc"
  },
  "dependencies": {
    "@azure/service-bus": "^7.1.0",
    "debug": "~4.3.1",
    "notepack.io": "~3.0.1",
    "socket.io-parser": "~4.2.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.0.0",
    "expect.js": "~0.3.1",
    "mocha": "^11.7.1",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
